record(mbbi, "$(BL):$(DEV):SensorType_RBV"){
  field(INP, "$(BL):$(DEV):SensorType_raw CPP")
  field(ZRST, "K Type")
  field(ONST, "J Type")
  field(TWST, "T Type")
  field(THST, "E Type")
  field(FRST, "N Type")
  field(FVST, "R Type")
  field(SXST, "S Type")
  field(SVST, "B Type")
  field(EIST, "L Type")
